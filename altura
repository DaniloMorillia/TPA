public class altura {

			public static void main(String[] args) {
		        double j= 134;
		        double p= 145; 
		        double i= 1;
		        
		       
		        while (j<p) {
		        
			        j = 2.5+j;
			        p = 2+p;
			        
			        i++;
		        }
		        System.out.println(" João levará "+i+ " anos para ficar mais alto que Pedro.");
		        }
		      
		    }

